            "_id": "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id": "info.connection",
            "type": "state",
            "common": {
                "role": "indicator.connected",
                "name": "If connected to CUL",
                "type": "boolean",
                "read": true
            },
            "native": {}
        },
        {
            "_id": "info.firmware",
            "type": "state",
            "common": {
                "name": "Firmware Version",
                "type": "string",
