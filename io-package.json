            },
            "1.0.3": {
                "en": "Bug fixes, small changes",
                "de": "Fehler beseitigt und Kleinigkeiten geändert"
            },
            "0.3.1": {
                "en": "add rawdata, chargecycle and actualArea",
                "de": "Rohdaten hinzugefügt"
            },
            "0.3.0": {
                "en": "add rawdata",
                "de": "Rohdaten hinzugefügt"
