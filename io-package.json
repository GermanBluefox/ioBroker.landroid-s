        "title": "Worx Landroid S 2017 ",
        "desc": {
            "en": "Control your Landroid S 2017",
            "de": "Steuert deinen Landroid S 2017",
            "ru": "Control your Landroid S 2017",
            "pt": "Control your Landroid S 2017",
            "fr": "Control your Landroid S 2017",
            "nl": "Control your Landroid S 2017"
        },
        "platform": "Javascript/Node.js",
        "mode": "daemon",
        "icon": "landroid-s.png",
        "enabled": true,
        "extIcon": "https://raw.githubusercontent.com/MeisterTR/iobroker.landroid-s/master/admin/landroid-s.png",
        "keywords": [
            "worx",
            "landroid s",
            "mower"
